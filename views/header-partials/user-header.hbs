<div class="thrown-topbar-area">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-lg-2"></div>
			<div class="col-lg-6">
				<div class="topbar-info">
					{{#each sites}}
					<span><i class="bi bi-geo-alt-fill"></i>{{this.Saddress}}</span>
					{{/each}}
				</div>
			</div>
			<div class="col-lg-4">
				<div class="topbar-social-item">
					<h4>FOLLOW US</h4>
					<ul>
						{{#each socials}}
						<li><a href="{{this.url}}"><i class="bi bi-{{this.iname}}"></i></a></li>
						{{/each}}

					</ul>
				</div>
			</div>
		</div>
	</div>
</div>


